function timkiemweb(n){var t=$(n).val();t.length>=3&&(annoidungweb_hientimkiem(),$("#timkiemtxtduoi").val(t),$("#timkiemtxtduoi").focus())}function TimKiemDuoi(){var n=$("#timkiemtxtduoi").val();$.ajax({type:"GET",url:"/Home/_KetQuaTimKiem",data:{tukhoa:n},beforeSend:function(){$("#timkiemkqchitiet").html(null);$("#loadingkqtk").removeClass("d-none")},success:function(n){$("#timkiemkqchitiet").html(n);$("#timkiemkqchitiet").removeClass("d-none")},complete:function(){$("#loadingkqtk").addClass("d-none")},error:function(n,t){alert(t)}})}function annoidungweb_hientimkiem(){$("#vungmot").addClass("d-none");$("#vunghai").addClass("d-none");$("#maps").addClass("d-none");$("#vungbon").addClass("d-none");$("#content").addClass("d-none");$("#list").addClass("d-none");$("#danhsach").addClass("d-none");$("#lichcongtac").addClass("d-none");$("#timkiem").removeClass("d-none");$(".has-search").addClass("d-none")}function hiennoidungweb_antimkiem(){$("#vungmot").removeClass("d-none");$("#vunghai").removeClass("d-none");$("#maps").removeClass("d-none");$("#vungbon").removeClass("d-none");$("#content").removeClass("d-none");$("#list").removeClass("d-none");$("#danhsach").removeClass("d-none");$("#lichcongtac").removeClass("d-none");$("#timkiem").addClass("d-none");$(".has-search").removeClass("d-none")}function resizeIframe(n){var t=$("#pageCount").val();console.log(t);t==0&&(t=1);n.style.height=t*1450+"px"}function nocontext(n){var t=n==null?event.srcElement.tagName:n.target.tagName;if(t=="IMG")return!1}function toggleButton(){$(window).scrollTop()>100?$(".TopButton").fadeIn():$(".TopButton").fadeOut()}$(function(){var n=window.location.href;$("#navbarText a").each(function(){n==this.href&&$(this).closest("li").addClass("active-li")})});$(".dropdown-menu a.dropdown-toggle").on("click",function(){$(this).next().hasClass("show")||$(this).parents(".dropdown-menu").first().find(".show").removeClass("show");var n=$(this).next(".dropdown-menu");n.toggleClass("show");$(this).parents("li.nav-item.dropdown.show").on("hidden.bs.dropdown",function(){$(".dropdown-submenu .show").removeClass("show")});return!1});var htmlapp=$(".ktcn_search_app").html();$(".ktcn_search").html(htmlapp);$("#btntimkiem").click(function(){TimKiemDuoi()});$("#timkiemtxtduoi").on("keyup",function(n){(n.key==="Enter"||n.keyCode===13)&&TimKiemDuoi()});$("#btnhuytimkiem").click(function(){hiennoidungweb_antimkiem();$("#timkiemtxt").val("")});$(document).ready(function(){var n=$(".owl-carousel11111");n.owlCarousel({items:5,loop:!0,margin:10,autoplay:!0,autoplayTimeout:5e3,autoplayHoverPause:!0,center:!0})});$(document).ready(function(){var n=$(".owl-carousel-video");n.owlCarousel({items:3,loop:!0,margin:10,autoplay:!0,autoplayTimeout:5e3,autoplayHoverPause:!1,center:!0})});$(document).ready(function(){var n=["bg-do","bg-cam","bg-xanhdam","bg-DarkGreen","bg-tim","bg-nau","bg-dc3545","bg-7aa734"];$(".khoimau").each(function(){var t=~~(Math.random()*n.length);$(this).addClass(n[t]);n.splice(t,1);n.length<1&&(n=["bg-do","bg-cam","bg-xanhdam","bg-DarkGreen","bg-tim","bg-nau","bg-dc3545","bg-7aa734"])})});$(function(){$(".mo-ta-tin-tuc").succinct({size:100})});$(document).ready(function(){$(".noidungbaidang img").removeAttr("style");$(".noidungbaidang img").addClass("img-fluid")});document.oncontextmenu=nocontext;jQuery(document).ready(function(n){toggleButton();n(window).scroll(toggleButton);n(".TopButton").click(function(){return n("html, body").animate({scrollTop:0},360),!1})})