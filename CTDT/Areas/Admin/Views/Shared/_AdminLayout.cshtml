@{
    var jsPath = Url.Content("~/Scripts/site.js");
    var version = System.IO.File.GetLastWriteTime(Server.MapPath("~/Scripts/site.js")).Ticks;
    var cssPath = Url.Content("~/Content/_site.css");
    var cssVersion = System.IO.File.GetLastWriteTime(Server.MapPath("~/Content/_site.css")).Ticks;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    @if (IsSectionDefined("LayoutHead"))
    {
        @RenderSection("LayoutHead")
    }
    <title>Quản trị - Ban khảo thí, kiểm định và đảm bảo chất lượng - TDMU</title>
    <!-- Favicon -->
    <link rel="shortcut icon" href="https://tdmu.edu.vn/hinh/Icon.png">

    <!-- page css -->
    <link href="~/Areas/assets/vendors/datatables/dataTables.bootstrap.min.css" rel="stylesheet">
    <link href="~/Areas/assets/vendors/select2/select2.css" rel="stylesheet" />
    <link href="~/Areas/assets/vendors/bootstrap-datepicker/bootstrap-datepicker.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/2.0.0/css/buttons.dataTables.min.css">
    <link href="https://fonts.googleapis.com/css2?family=Be+Vietnam+Pro:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
    <!-- Core css -->
    <link href="~/Areas/assets/css/app.min.css" rel="stylesheet" />
    <link href="~/Areas/assets/vendors/datatables/dataTables.bootstrap.min.css" rel="stylesheet">
    <style>
        .ui-datepicker select.ui-datepicker-month, .ui-datepicker select.ui-datepicker-year {
            width: 50%;
        }
    </style>
    <link rel="stylesheet" href="@cssPath?v=@cssVersion" />
</head>
<body>
    <div class="app">

        <div class="layout">
            <!-- Header START -->
            @Html.Partial("_AdminHeaderPartial")
            <!-- Header END -->
            <!-- Side Nav START -->
            @Html.Partial("_AdminNavPartial")
            <!-- Side Nav END -->
            <!-- Page Container START -->
            <div class="page-container">
                <!-- Content Wrapper START -->
                @RenderBody()
                <!-- Content Wrapper END -->
                <!-- Footer START -->
                <!-- Footer END -->
                @Html.Partial("_AdminFooterPartial")
            </div>
            <!-- Page Container END -->
        </div>
    </div>



    <!-- Core Vendors JS -->
    <!-- page js -->
    <!-- Core JS -->
    @Scripts.Render("~/bundles/jquery")
    <script src="https://cdn.tiny.cloud/1/3o2m32iii9o55ujnm4ghu67cv0wkw35uq2g1flnfckh6237l/tinymce/7/tinymce.min.js" referrerpolicy="origin"></script>
    <script src="@jsPath?v=@version"></script>
    @Scripts.Render("~/bundles/CTDT/js")
    @RenderSection("scriptsAdmin", required: false)
</body>
</html>


